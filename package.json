{
  "name": "game-iframe-app",
  "version": "1.0.0",
  "description": "Angular app to render games in iframe",
  "main": "index.js",
  "scripts": {
    "start": "ng serve",
    "start:proxy": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "test": "ng test",
    "proxy": "node proxy-server.js",
    "dev": "concurrently \"npm run start:proxy\" \"npm run proxy\"",
    "deploy": "npm run build:prod && npm run proxy"
  },
  "dependencies": {
    "@angular/common": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "node-fetch": "^3.3.2",
    "rxjs": "~7.8.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "typescript": "~5.2.0"
  },
  "proxyDependencies": {
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "cors": "^2.8.5",
    "node-fetch": "^3.3.2",
    "concurrently": "^8.2.2"
  }
}
